\$entityManager = new \MulerTech\Database\ORM\EntityManager(
    new \MulerTech\Database\Database\Interface\PhpDatabaseManager(
        new \MulerTech\Database\Database\Interface\PdoConnector(new \MulerTech\Database\Database\MySQLDriver()), 
        []
    ),
    new \MulerTech\Database\Core\Cache\MetadataCache(null, dirname(__FILE__) . '/tests/Files/Entity')
);
\$informationSchema = new \MulerTech\Database\Schema\Information\InformationSchema(\$entityManager->getEmEngine());
\$schemaComparer = new \MulerTech\Database\Schema\Diff\SchemaComparer(\$informationSchema, \$entityManager->getMetadataCache(), 'db');
\$diff = \$schemaComparer->compare();
echo 'Has differences: ' . (\$diff->hasDifferences() ? 'true' : 'false') . PHP_EOL;
echo 'Tables to create: ' . json_encode(\$diff->getTablesToCreate()) . PHP_EOL;
echo 'Tables to drop: ' . json_encode(\$diff->getTablesToDrop()) . PHP_EOL;
echo 'Columns to add: ' . json_encode(\$diff->getColumnsToAdd()) . PHP_EOL;
echo 'Columns to modify: ' . json_encode(\$diff->getColumnsToModify()) . PHP_EOL;
echo 'Columns to drop: ' . json_encode(\$diff->getColumnsToDrop()) . PHP_EOL;
echo 'Foreign keys to add: ' . json_encode(\$diff->getForeignKeysToAdd()) . PHP_EOL;
echo 'Foreign keys to drop: ' . json_encode(\$diff->getForeignKeysToDrop()) . PHP_EOL;
"
